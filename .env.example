# use hash mode for router (true | false)
VITE_ROUTER_USE_HASH=true
# is sign with wallet disabled (true | false) 
VITE_WALLET_DISABLE_SIGN=true
# wallet sign timeout in seconds
VITE_WALLET_SIGN_TIMEOUT_IN_SEC=60
# slow down wallet operations by given milliseconds (for slowing down UI updates)
VITE_WALLET_SLOW_DOWN_IN_MS=1000
# post login redirect path (empty string for home page)
VITE_POST_LOGIN_REDIRECT_PATH=''

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173
# OAuth scopes (includes profile photos via 'profile' scope)
VITE_GOOGLE_SCOPE=openid email profile

# GitHub OAuth Configuration
VITE_GITHUB_CLIENT_ID=your-github-client-id
VITE_GITHUB_REDIRECT_URI=http://localhost:5173/auth/callback/github
# OAuth scopes (avatar_url available as public data with any scope)
VITE_GITHUB_SCOPE=user:email
